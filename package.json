{
  "name": "personal-site",
  "version": "4.0.0",
  "homepage": "http://piyushdatta.github.io/personal_website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PiyushDatta/personal_website"
  },
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "gen_resume": "python convert_resume_to_json.py",
    "lint_all": "eslint .",
    "prebuild": "bun run gen_resume && bun run lint_fix && bun run lint_all",
    "prestart": "bun run gen_resume && bun run lint_fix && bun run lint_all",
    "predeploy": "bun run gen_resume && bun run lint_fix && bun run lint_all",
    "build": "rimraf ./build && react-scripts build",
    "start": "react-scripts start",
    "deploy": "gh-pages -d build",
    "analyze": "bun run build && source-map-explorer build/static/js/*.chunk.js",
    "lint": "eslint \"**/*.js\"",
    "lint_fix": "eslint --fix .",
    "before_commit": "bun run format && bun run lint_fix && bun run build && react-snap",
    "test": "npx jest",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "dayjs": "^1.11.10",
    "markdown-to-jsx": "^7.4.7",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
